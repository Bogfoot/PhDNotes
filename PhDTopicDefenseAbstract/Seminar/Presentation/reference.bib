@article{QuCal,
title = {Single photon sources for absolute radiometry – A review about the current state of the art},
journal = {Measurement: Sensors},
volume = {18},
pages = {100219},
year = {2021},
issn = {2665-9174},
doi = {https://doi.org/10.1016/j.measen.2021.100219},
url = {https://www.sciencedirect.com/science/article/pii/S2665917421001823},
author = {Stefan Kück},
keywords = {Quantum radiometry, Quantum metrology, Single-photon source, Single-photon detector, Quantum dot},
abstract = {In this presentation a review about the current state of the art concerning the application of deterministic single photon sources for absolute radiometry, i.e. the calibration of single photon detectors, is given. At the Physikalisch-Technische Bundesanstalt, the German national Metrology institute, an extensive research is carried out to optimize single photon sources for this purpose. Different single photon sources are currently under investigation, these are sources based on defect centres in (nano-)diamonds, on molecules and on semiconductor quantum dots.}
},
