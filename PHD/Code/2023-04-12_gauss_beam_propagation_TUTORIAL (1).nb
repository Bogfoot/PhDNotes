    RowBox[{"w1", "=", 
     RowBox[{"0.8153085635244979", " ", "/", " ", "1000"}]}], ";", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{"z2", "=", 
     RowBox[{"888", " ", 
