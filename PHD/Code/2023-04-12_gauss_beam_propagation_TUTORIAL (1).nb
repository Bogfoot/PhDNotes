                RowBox[{"\[Pi]", " ", 
                 SuperscriptBox["w0", "2"]}]], ")"}], "2"], " "}]]}]}], ",", 
         RowBox[{"w2", "==", 
          RowBox[{"w0", " ", 
           SqrtBox[
            RowBox[{"1", "+", 
             SuperscriptBox[
              RowBox[{"(", 
               FractionBox[
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{"z2", "-", "zwaist"}], ")"}], " ", "\[Lambda]"}], 
                RowBox[{"\[Pi]", " ", 
                 SuperscriptBox["w0", "2"]}]], ")"}], "2"]}]]}]}]}], "}"}], 
       ",", 
       RowBox[{"{", 
        RowBox[{"w0", ",", "zwaist"}], "}"}]}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"Print", "[", "solutions", "]"}]}]}], "\[IndentingNewLine]", 
  "]"}]], "Input",
