 FormBox[
  RowBox[{
   RowBox[{"w", "(", 
    RowBox[{"z", "-", 
     SubscriptBox["z", "0"]}], ")"}], " ", "=", " ", 
   RowBox[{
    SubscriptBox["w", "0"], 
    SqrtBox[
     RowBox[{"1", "+", 
      SuperscriptBox[
       RowBox[{"(", 
        FractionBox[
         RowBox[{"z", "-", 
          SubscriptBox["z", "0"]}], 
         SubscriptBox["z", "R"]], ")"}], "2"]}]]}]}], 
  TraditionalForm]], "Equation",
