   RowBox[{"A_", ",", "qOld_"}], "]"}], ":=", 
  FractionBox[
   RowBox[{
    RowBox[{
     RowBox[{"A", "[", 
      RowBox[{"[", 
       RowBox[{"1", ",", "1"}], "]"}], "]"}], " ", "qOld"}], "+", 
    RowBox[{"A", "[", 
     RowBox[{"[", 
      RowBox[{"1", ",", "2"}], "]"}], "]"}]}], 
   RowBox[{
    RowBox[{
     RowBox[{"A", "[", 
      RowBox[{"[", 
       RowBox[{"2", ",", "1"}], "]"}], "]"}], " ", "qOld"}], " ", "+", " ", 
    RowBox[{"A", "[", 
     RowBox[{"[", 
      RowBox[{"2", ",", "2"}], "]"}], "]"}]}]]}]], "Input",
 InitializationCell->True,
 CellLabel->"In[50]:=",ExpressionUUID->"c7e80495-e7a2-446f-8d2c-b66a49029b84"],

