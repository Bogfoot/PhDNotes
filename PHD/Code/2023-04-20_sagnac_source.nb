Cell[BoxData[
 FormBox[
  RowBox[{"\[CapitalDelta]k", "\[Equal]", 
   RowBox[{
    RowBox[{
     SubscriptBox[
      RowBox[{"(", 
       FractionBox[
        RowBox[{"\[PartialD]", "\[CapitalDelta]k"}], 
        RowBox[{"\[PartialD]", 
         SubscriptBox["\[Omega]", "s"]}]], ")"}], 
      SubscriptBox["\[Omega]", "p"]], 
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["\[Omega]", "s"], "-", 
       SubscriptBox["\[Omega]", 
        RowBox[{"s", ",", "0"}]]}], ")"}]}], " ", "+", 
    RowBox[{
     FractionBox["1", "2"], " ", 
     SubscriptBox[
      RowBox[{"(", 
       FractionBox[
        RowBox[{
