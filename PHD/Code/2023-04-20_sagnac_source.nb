   3.61260285168694*^9, 3.612602902216988*^9}, {3.612603084210451*^9, 
   3.612603141379106*^9}, {3.8836463452185445`*^9, 3.883646402735465*^9}, {
   3.8841570218247805`*^9, 3.8841570399270773`*^9}, {3.8844095865964174`*^9, 
   3.884409613063284*^9}},ExpressionUUID->
