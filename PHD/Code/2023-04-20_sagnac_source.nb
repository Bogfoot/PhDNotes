        RowBox[{"\[PartialD]", 
         SubsuperscriptBox["\[Omega]", "s", "2"]}]], ")"}], 
      SubscriptBox["\[Omega]", "p"]], 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["\[Omega]", "s"], "-", 
        SubscriptBox["\[Omega]", 
         RowBox[{"s", ",", "0"}]]}], ")"}], "2"]}], " ", "+", 
    RowBox[{
     SubscriptBox[
      RowBox[{"(", 
       FractionBox[
        RowBox[{"\[PartialD]", "\[CapitalDelta]k"}], 
        RowBox[{"\[PartialD]", 
         SubscriptBox["\[Omega]", "p"]}]], ")"}], 
      SubscriptBox["\[Omega]", "s"]], " ", 
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["\[Omega]", "p"], "-", 
       SubscriptBox["\[Omega]", 
        RowBox[{"p", ",", "0"}]]}], ")"}]}], " ", "+", 
    RowBox[{
     FractionBox[
      RowBox[{"\[PartialD]", "\[CapitalDelta]k"}], 
      RowBox[{"\[PartialD]", "T"}]], " ", 
     RowBox[{"(", 
      RowBox[{"T", "-", 
       SubscriptBox["T", "0"]}], ")"}]}]}]}], TraditionalForm]], "Equation",
