Cell["\<\
The SPDC bandwidth is determined from the phase mismatch of the nonlinear \
process. The most important factors (that we can control) here are the \
crystal lengths and the temperature:\
\>", "Text",
 CellChangeTimes->{{3.826344591758605*^9, 
  3.826344661011307*^9}},ExpressionUUID->"5472ae6b-f796-471e-8d02-\
1e1b441a0db1"],

Cell[BoxData[
 FormBox[
  RowBox[{"\t", 
   RowBox[{"\[CapitalDelta]k", "\[Equal]", 
    RowBox[{
     RowBox[{
      SubscriptBox[
       RowBox[{"(", 
        FractionBox[
         RowBox[{"\[PartialD]", "\[CapitalDelta]k"}], 
         RowBox[{"\[PartialD]", 
          SubscriptBox["\[Omega]", "s"]}]], ")"}], 
       SubscriptBox["\[Omega]", "p"]], 
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["\[Omega]", "s"], "-", 
        SubscriptBox["\[Omega]", 
         RowBox[{"s", ",", "0"}]]}], ")"}]}], " ", "+", 
     RowBox[{
      FractionBox["1", "2"], " ", 
      SubscriptBox[
       RowBox[{"(", 
        FractionBox[
         RowBox[{
          SuperscriptBox["\[PartialD]", "2"], "\[CapitalDelta]k"}], 
         RowBox[{"\[PartialD]", 
          SubsuperscriptBox["\[Omega]", "s", "2"]}]], ")"}], 
       SubscriptBox["\[Omega]", "p"]], 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["\[Omega]", "s"], "-", 
         SubscriptBox["\[Omega]", 
          RowBox[{"s", ",", "0"}]]}], ")"}], "2"]}], " ", "+", 
     RowBox[{
      SubscriptBox[
       RowBox[{"(", 
        FractionBox[
         RowBox[{"\[PartialD]", "\[CapitalDelta]k"}], 
         RowBox[{"\[PartialD]", 
          SubscriptBox["\[Omega]", "p"]}]], ")"}], 
       SubscriptBox["\[Omega]", "s"]], " ", 
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["\[Omega]", "p"], "-", 
        SubscriptBox["\[Omega]", 
         RowBox[{"p", ",", "0"}]]}], ")"}]}], " ", "+", 
     RowBox[{
      FractionBox[
       RowBox[{"\[PartialD]", "\[CapitalDelta]k"}], 
       RowBox[{"\[PartialD]", "T"}]], " ", 
      RowBox[{"(", 
       RowBox[{"T", "-", 
        SubscriptBox["T", "0"]}], ")"}]}]}]}]}], TraditionalForm]], "Equation",
