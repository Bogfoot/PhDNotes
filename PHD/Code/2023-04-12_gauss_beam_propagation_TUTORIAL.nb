      "\[IndentingNewLine]", "z1plotmm", ",", "z2plotmm", ",", "z3plotmm", 
      ",", "\[IndentingNewLine]", "qOldRED", ",", "q1newRED", ",", "q2newRED",
       ",", "q3newRED", ",", "q4newRED", ",", "q5newRED", ",", "q6newRED", 
