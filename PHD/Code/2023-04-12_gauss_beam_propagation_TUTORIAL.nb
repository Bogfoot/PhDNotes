Cell[BoxData[
 RowBox[{"Module", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
    "\[Lambda]", ",", "z1", ",", "w1", ",", "z2", ",", "w2", ",", 
     "solutions"}], "}"}], ",", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"\[Lambda]", "=", 
     RowBox[{"775", " ", 
      SuperscriptBox["10", 
       RowBox[{"-", "9"}]]}]}], ";", "\[IndentingNewLine]", 
    "\[IndentingNewLine]", 
    RowBox[{"z1", "=", 
     RowBox[{"261", " ", 
      SuperscriptBox["10", 
       RowBox[{"-", "3"}]]}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"w1", "=", 
     RowBox[{"0.815309", " ", 
      SuperscriptBox["10", 
       RowBox[{"-", "3"}]]}]}], ";", "\[IndentingNewLine]", 
    "\[IndentingNewLine]", 
    RowBox[{"z2", "=", 
     RowBox[{"888", " ", 
      SuperscriptBox["10", 
       RowBox[{"-", "3"}]]}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"w2", "=", 
     RowBox[{"0.952767", " ", 
      SuperscriptBox["10", 
       RowBox[{"-", "3"}]]}]}], ";", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"w", "[", 
      RowBox[{"z_", ",", "w0_", ",", "\[Lambda]_"}], "]"}], ":=", 
     RowBox[{"w0", " ", 
      SqrtBox[
       RowBox[{"1", "+", 
        SuperscriptBox[
         RowBox[{"(", 
          FractionBox[
           RowBox[{"z", " ", "\[Lambda]"}], 
           RowBox[{"\[Pi]", " ", 
            SuperscriptBox["w0", "2"]}]], ")"}], "2"]}]]}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"solutions", "=", 
     RowBox[{"NSolve", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"w1", "==", 
          RowBox[{"w0", " ", 
           SqrtBox[
            RowBox[{"1", "+", 
             SuperscriptBox[
              RowBox[{"(", 
               FractionBox[
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{"z1", "-", "zwaist"}], ")"}], "\[Lambda]"}], 
                RowBox[{"\[Pi]", " ", 
                 SuperscriptBox["w0", "2"]}]], ")"}], "2"]}]]}]}], ",", 
         RowBox[{"w2", "==", 
          RowBox[{"w0", " ", 
           SqrtBox[
            RowBox[{"1", "+", 
             SuperscriptBox[
              RowBox[{"(", 
               FractionBox[
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{"z2", "-", "zwaist"}], ")"}], " ", "\[Lambda]"}], 
                RowBox[{"\[Pi]", " ", 
                 SuperscriptBox["w0", "2"]}]], ")"}], "2"]}]]}]}]}], "}"}], 
       ",", 
       RowBox[{"{", 
        RowBox[{"w0", ",", "zwaist"}], "}"}]}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"Print", "[", "solutions", "]"}]}]}], "\[IndentingNewLine]", 
  "]"}]], "Input",
